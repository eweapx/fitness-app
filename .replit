modules = ["flutter", "nodejs-20", "dart", "python-3.11"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run Tests"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Fitness App Server"

[[workflows.workflow]]
name = "Run Tests"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx jest"

[[workflows.workflow]]
name = "Flutter Web"
author = 40359398
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter pub get"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter run -d web-server --web-port=5000 --web-hostname=0.0.0.0"

[[workflows.workflow]]
name = "Flutter Web Server"
author = 40359398
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter build web"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server.js"

[[workflows.workflow]]
name = "Fitness App Server"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node basic-server.js"
waitForPort = 5000

[deployment]
run = ["sh", "-c", "flutter run -d web-server --web-port=5000 --web-hostname=0.0.0.0"]
build = ["sh", "-c", "flutter build web"]

